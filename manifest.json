{
  "manifest_version": 3,
  "name": "Google Watchlist Quick Open",
  "version": "0.0.0.1",
  "description": "One-click shortcut to open Googleâ€™s Saved page and jump straight into your Search watchlist.",
  "action": {
    "default_title": "Google Watchlist Quick Open",
